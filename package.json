{
  "name": "tdx-rail-tra-cli",
  "version": "0.1.0",
  "description": "Taiwan Railway (TRA) CLI tool powered by TDX API",
  "type": "module",
  "bin": {
    "tra": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user/tdx-rail-tra-cli.git"
  },
  "homepage": "https://github.com/user/tdx-rail-tra-cli#readme",
  "bugs": {
    "url": "https://github.com/user/tdx-rail-tra-cli/issues"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "build:binary": "bun build src/index.ts --compile --outfile dist/tra",
    "build:binary:macos-arm64": "bun build src/index.ts --compile --target=bun-darwin-arm64 --outfile dist/tra-macos-arm64",
    "build:binary:macos-x64": "bun build src/index.ts --compile --target=bun-darwin-x64 --outfile dist/tra-macos-x64",
    "build:binary:linux-x64": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile dist/tra-linux-x64",
    "build:binary:linux-arm64": "bun build src/index.ts --compile --target=bun-linux-arm64 --outfile dist/tra-linux-arm64",
    "build:binary:windows-x64": "bun build src/index.ts --compile --target=bun-windows-x64 --outfile dist/tra-windows-x64.exe",
    "build:binary:all": "npm run build:binary:macos-arm64 && npm run build:binary:macos-x64 && npm run build:binary:linux-x64 && npm run build:binary:linux-arm64 && npm run build:binary:windows-x64",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "dotenv": "^16.4.7",
    "ofetch": "^1.4.1",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "msw": "^2.7.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "taiwan",
    "railway",
    "tra",
    "tdx",
    "cli",
    "timetable",
    "train",
    "transport",
    "api",
    "tpass",
    "booking",
    "schedule",
    "fare",
    "ai-agent",
    "automation"
  ],
  "author": "",
  "license": "MIT"
}
